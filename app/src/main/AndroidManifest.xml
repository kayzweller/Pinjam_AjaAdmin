<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xoxltn.pinjam_ajaadmin">

    <uses-feature
        android:name="android.hardware.fingerprint"
        android:required="true" />

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SubmenuReturnActivity"></activity>
        <activity android:name=".SubmenuDetailPeminjamActivity" />
        <activity android:name=".SubmenuDetailPendanaActivity" />
        <activity android:name=".SubmenuApprovalPendanaActivity" />
        <activity android:name=".SubmenuApprovalPeminjamActivity" />
        <activity android:name=".SubmenuDetailActivity" />
        <activity android:name=".SubmenuApprovalActivity" />
        <activity android:name=".SubmenuRequestPeminjamActivity" />
        <activity android:name=".SubmenuRequestActivity" />
        <activity android:name=".MainDashboardActivity" />
        <activity android:name=".MainLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>